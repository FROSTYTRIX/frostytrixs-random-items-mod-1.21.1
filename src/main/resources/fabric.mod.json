{
	"schemaVersion": 1,
	"id": "randomitemsmod",
	"version": "${version}",
	"name": "Frostytrix's Random Items Mod",
	"description": "A mod that adds many random items. The idea came from Daniel Krafft's Designing Custom Minecraft Items (Some of the items comes from his videos)",
	"authors": [
		"FROSTYTRIX"
	],
	"contact": {
		"homepage": "https://x.com/FROSTYTRIX__",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/frostytrixs-random-items-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.frostytrix.randomitems.RandomItemsMod"
		],
		"fabric-datagen": [
          "net.frostytrix.randomitems.RandomItemsDataGenerator"
		],
		"client": [
			"net.frostytrix.randomitems.RandomItemClient"
		]
	},
	"mixins": [
		"frostytrixs-random-items-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.7",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}